{
  "problemMatcher": [
    {
      "owner": "csval_problem",
      "severity": "error",
      "pattern": [
        {
          "regexp": "^File:\\s*(.*)$",
          "file": 1
        },
        {
          "regexp": "^\\s*(Error|Warning):\\s*(.*):$",
          "severity": 1,
          "code": 2
        },
        {
          "regexp": "^\\s+-\\s*Row\\s+(\\d+):\\s*(.*)$",
          "line": 1,
          "message": 2,
          "loop": true
        }
      ]
    }
  ]
}
