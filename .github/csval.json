{
  "problemMatcher": [
    {
      "owner": "csval_problems",
      "severity": "error",
      "pattern": [
        {
          "regexp": "^\\s*(?>File:)\\s*([^\\s].*)$",
          "file": 1
        },
        {
          "regexp": "^\\s*(Error|Warning):\\s*(.*):$",
          "severity": 1,
          "code": 2
        },
        {
          "regexp": "^\\s+(?>-\\s*Row\\s+(\\d+):)\\s*(.*)$",
          "line": 1,
          "message": 2,
          "loop": true
        }
      ]
    },
    {
      "owner": "csval_info",
      "severity": "info",
      "pattern": [
        {
          "regexp": "^\\s*(?>File:)\\s*([^\\s].*)$",
          "file": 1
        },
        {
          "regexp": "^(The CSV file meets all validation checks\\.)$",
          "message": 1
        }
      ]
    }
  ]
}
