{
  "problemMatcher": [
    {
      "owner": "csval_error",
      "severity": "error",
      "pattern": [
        {
          "regexp": "^\\s*(?>File:)\\s*([^\\s].*)$",
          "file": 1
        },
        {
          "regexp": "^\\s*(Error):(.*):$",
          "code": 2
        },
        {
          "regexp": "^\\s+(?>-\\s*Row\\s+(\\d+):)\\s*(.*)$",
          "line": 1,
          "message": 2,
          "loop": true
        }
      ]
    }
  ]
}
